                                    Content="F12" />
                            </UniformGrid>
                        </Grid>
                        <Border.InputBindings>
                            <KeyBinding Key="F1" Command="{TemplateBinding F01ButtonCommand}" />
                            <KeyBinding Key="F2" Command="{TemplateBinding F02ButtonCommand}" />
                            <KeyBinding Key="F3" Command="{TemplateBinding F03ButtonCommand}" />
